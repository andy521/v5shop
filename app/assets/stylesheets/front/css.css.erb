  	}
/*服务流程*/
.fwlc{
	width:100%;
	height:330px;
	margin:0px auto;
	background:url(<%= asset_path 'front/bg2.jpg' %>) repeat-x;	
	}
.fwlccon{
	width:1000px;
	height:330px;
	margin:0px auto;
	background-color:#fff; 
	}
.fwlccon img{
	width:1000px;
