﻿@charset "utf-8";
/* CSS Document */
*{ margin:0; padding:0;}
/*container*/
.container1{
	  width:100%;
      height:auto;
      margin:0px auto;
	  background-color:#FFF;
	 }
/*head*/
.head{
	 width:100%;
     height:32px;
     margin:0px auto;
	 background-color:#f3f3f3;
	}
.headcon{
	width:1000px;
	height:32px;
	margin:0 auto; 
	}	
.headcon_l{
	width:500px;
	height:32px;
	line-height:32px;
	float:left;
	font-size:12px;
	color:#7c7979;
	}	
.headcon_r{
	width:500px;
	height:32px;
	line-height:32px;
	float:right;
	text-align:right;
	color:#7c7979;
	}	
.headcon_r a{
	font-size:12px;
	color:#7c7979;
	}	
.headcon_r a:hover{
	font-size:12px;
	color:#ee8d00;
	text-decoration:none;
	}	
/*head*/
/*top*/
.top{
	width:100%;
	height:106px;
	margin:0 auto;
	background-color:#fff;
	}	
.topcon{
	width:1000px;
	height:106px;
	margin:0 auto; 
	}	
.topcon_l{
	width:199px;
	height:106px;
	float:left;
	/* font-family:"微软雅黑"; */
	color:#f28d01;
	font-size:50px;
	line-height:106px;
	}
.topcon_r{
	width:600px;
	height:106px;
	float:right;  
	}	
.dh{
	width:251px;
	height:30px;
	float:right;
	margin:10px 0 0 0;
	}
.menu{
	width:600px;
	height:38px;
	text-align:right;   
	}	
.menu ul{
	margin:0px;
	padding:0px;
	}
.menu ul li{
	width:120px;
	height:38px;
	float:left;	
	line-height:38px;
	margin:12px 0 0 0; 
	}
.menu ul li a{
	font-size:16px;
	color:#ee8d00;
	/* font-family:"微软雅黑"; */
	font-weight:bold;
	}	
.menu ul li a:hover{
	text-decoration:none;
	color:#e66801;
	cursor:pointer;
	}	
/*banner*/
.banner{
	width:100%;
	height:500px;
	margin:0px auto;
	background:url(<%= asset_path 'front/banner1.jpg' %>) no-repeat;
	}
.bannercon{
	width:1000px;
	height:500px;
	margin:0px auto; 
	}
.bannercon_l{
	width:580px;
	height:500px;
	float:left;
	}
.bannercon_l h1{
	/* font-family:"微软雅黑"; */
	color:#ff7e12;
	font-size:46px;
	text-align:center;
	font-weight:bold;
	line-height:220px; 
	}
.bannercon_l p{
	width:333px;
	height:90px;
	margin:20px auto;
	}
.bannercon_r{
	width:300px;
	height:270px;
	float:right;
	padding:230px 0 0 100px;
	font-size:16px;
	/* font-family:"微软雅黑"; */
	font-weight:bold;
	line-height:50px; 
	}
	.ewm-text{
		font-size:16px;
		/* font-family:"微软雅黑"; */
		font-weight:bold;
		margin-bottom: 10px;
	}
/*服务项目*/
.fwxm{
	width:100%;
	height:469px;
	margin:0px auto;
	background:url(<%= asset_path 'front/bg1.jpg' %>) repeat-x;	
	}
.fwxmcon{
	width:1000px;
	height:469px;
	margin:0px auto;
	background-color:#f1f1f1;  
	}
.tit1{
	width:1000px;
	height:100px;
	/* font-family:"微软雅黑"; */
	font-size:22px;
	color:#444444;
	text-align:center;
	line-height:100px;
	background:url(<%= asset_path 'front/titx.png' %>) no-repeat;
	}
.fw1{
	width:300px;
	height:110px;
	float:left;
	margin:20px 18px 20px 15px;
	}
.fw1_l{
	width:110px;
	height:110px;
	float:left;
	}
.fw1_r{
	width:170px;
	height:110px;
	float:right; 
	}
.fw1_r h1{
	width:170px;
	height:40px;
