	}
.fwcncon ul{ 
   width:1000px; 
   height:250px;
   margin:20px 0 0 0;
   zoom:1;  
   }
.fwcncon ul li{ 
   width:236px; 
   height:250px; 
   float:left;
   margin:0 8px 0 6px;
  	}
.fwcncon ul li img{
   width:236px; 
