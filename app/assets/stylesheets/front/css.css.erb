/*服务案例*/
.fwal{
	width:100%;
	height:426px;
	margin:0px auto;
	background:url(<%= asset_path 'front/bg2.jpg' %>) repeat-x;	
	}
.fwalcon{
	width:1000px;
	height:426px;
	margin:0px auto;
	background-color:#fff; 
	}
.fwalcon ul{ 
   width:1000px; 
   height:215px;
   margin:20px 0 0 0;
