	margin:20px auto;
	}
/*footer*/
.footer{
	width:100%;
	height:130px;
	margin:0px auto;
	border-top:#ff7e12 5px solid;
	background-color:#FFF;
	}
.footercon{
	width:1000px;
	height:130px;
	margin:0px auto; 
	}
.footer_links{
	width:1000px;
	height:60px;
	color:#333333;
	text-align:center;
	}
.footer_links a{
	font-size:12px;
	color:#333333;
	line-height:60px;
	}	
.footercon p{
  font-size:12px;
  color:#333333;
  text-align:center;
  line-height:40px;
   }

/*修改bootstrap样式*/

.input-group-addon{
	padding: 10px 12px;
	border-radius: 2px;
}
.form-control{
	height: 40px;
	border-radius: 2px;
}


.login-footer-label{
	font-size:11px;
	padding-right:0;
}
.login-footer-label a{
	color:#FFB06A;
}
.top-btn{
	border: 0px solid transparent;
	background-color: #F3F3F3;
}
.top-dropdown-menu{
	font-size: 12px;
}
.panel{
	border-radius: 2px;
}
.btn{
	border-radius: 2px;
}
.input-group-addon:not(:first-child):not(:last-child),
.input-group-btn:not(:first-child):not(:last-child),
.input-group .form-control:not(:first-child):not(:last-child){
	border-top-left-radius: 2px;
	border-bottom-left-radius: 2px;
	border-bottom-right-radius: 0;
	border-top-right-radius: 0;
}
.panel-default>.panel-heading{
	background-color:#fff;
}
hr {
	margin-top: 5px;
	margin-bottom: 10px;
	border: 0;
	border-top: 1px solid #DFDFDF;
}
.checkbox-inline{
	padding-left: 0;
	width:29.35%;
}
/*服务列表*/
.lab-txt{
	/* font-family:"Microsoft YaHei"; */
	color:#ff7e12;
	font-size:23px;
	font-weight:bold;
	line-height:100px;
}
.caption h4{
	font-size: 16px;
}
.caption p{
	line-height: 30px;
	color: #858585;
}
.caption .btnp{
	line-height:25px;
	margin-top: 2px;
}
.caption .btnp span{
	/* font-family:"Microsoft YaHei"; */
	font-size: 14px;
	font-weight:bold;
	color:#ff7e12;
}

.caption .btnp a{
	/* font-family:"Microsoft YaHei"; */
	background-color: #FE7E11;
	padding: 3px 6px;
	font-size: 11px;
	float: right;
}
.thumbnail img{
	padding: 4px;
}
.filtrate{
	line-height: 100px;
}
.filtrate a{
	/* font-family:"Microsoft YaHei"; */
	background-color: #E7E7E7;/*#FE7E11;*/
	border: 1px #E7E7E7 solid;
	color:#000;
	padding: 3px 20px;
	border-radius: 2px;
}
.filtrate a:hover{
	background-color: #FE7E11;
	border: 1px #FE7E11 solid;
}
.filtrate .pitch{
	background-color: #FE7E11;
	border: 1px #FE7E11 solid;
	color: white;
}

.next-banner{
	width: 1000px;
	padding: 0 0;
	text-align: center;
}

.bs-container{
	padding: 0 0;
	width: 1000px;
	margin-top: 10px;
	margin-bottom: 10px;
}

.container-bg{
	background-color: #F3F3F3;
	line-height: 100px;
	margin-top: 60px;
}

/* 购物车 头*/
.shead{
	background-color: #F3F3F3;
	margin: 0 0;
	height: 30px;
	padding: 8px 0;
	font-size: 11px;
}
.shead .first{
	border-left: none;
	text-align: left;
	padding-left: 30px;
}
.shead div{
	border-left: #d8d8d8 1px solid;
	text-align: center;
}
/* 购物车 列表*/
.slist{
	margin: 0 15px;
	line-height: 130px;
	border-bottom: #DFDFDF 1px solid;
	padding: 10px 0;
}
.slist .first{
	text-align: left;
	padding-left: 40px;
}
.slist div{
	text-align: center;
}
.cattitle{
	margin-left: 25px;
	font-size: 20px;
	text-decoration:none;
	color:#333;
}
.cart-btn{
	padding: 0;
	border: none;
}
.cart-btn:hover{
	padding: 0 1px;
	border: #333 1px solid;
	border-radius: 2px 2px;
}
/*购物车价格样式*/
.cart-price{
	float:right;
	line-height:100px;
	margin-right:50px;
}
.cart-price .first{
	margin-right:30px;
}
.cart-price #total{
	font-size:30px;
	color:#FE7E11;
	margin-right:3px;
}
.cart-price .last{
	font-size:18px;
	color:#FE7E11;
}
/** spinner **/
.spinner-buttons.btn-group-vertical .btn {
    height: 17px;
    margin: 0;
    padding-left: 6px;
    padding-right: 6px;
    text-align: center;
    width: 22px;
}

.spinner-buttons.btn-group-vertical .btn i {
    margin-top: -3px;
}
.spinner-buttons.btn-group-vertical .btn:first-child {
    border-radius: 0 4px 0 0 !important;
    -webkit-border-radius: 0 4px 0 0 !important;
}

.spinner-buttons.btn-group-vertical .btn:last-child {
    border-radius: 0 0 4px !important;
    -webkit-border-radius: 0 0 4px !important;
}
.spinner .spinner-input{
	float: none;
	height: 34px;
	text-align: center;
}
.area-select{
	display:inline;
	height: 34px;
	width: 150px;
}

/*订单确认页面*/
.box-title{
	padding: 8px 15px;
}

.box-title .box-label{
	color: #FE7E11;
	line-height: 30px;
}

.address{
	width: 32.3%;
	margin:5px 5px;
	padding-top: 20px;
	color:#333333;
	height:180px;
	border:1px #DFDFDF solid;
	background-color: #FAFAFA;
	border-radius: 2px;
}
.address:hover{
	color:#333333;
	background-color: #F3F3F3;
	text-decoration: none;
}
.address:focus{
	color:#333333;
}
.selected{
	border:1px #FE7E11 solid;
	background-color: #FFFFFF;
}
.selected:hover{
	background-color: #FFFFFF;
}

.edit-btn{
	margin-top: 20px;
	display: none;
}

.dateintro{
	margin-bottom: 50px;
}