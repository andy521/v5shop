<%= form_for @admin_division,html:{class:'form-horizontal',role:'form'} do |f| %>
      <% if @admin_division.errors.any? %>
        <div id="error_explanation">
          <h2><%= pluralize(@admin_division.errors.count, "error") %> prohibited this admin_division from being saved:</h2>

          <ul>
          <% @admin_division.errors.full_messages.each do |message| %>
            <li><%= message %></li>
          <% end %>
          </ul>
        </div>
      <% end %>
      <div class="form-group">
          <label for="inputEmail3" class="col-sm-2 control-label">父区划</label>
          <%= f.hidden_field :parent_id %>
          <%= f.hidden_field :parent_ids %>
          <div class="col-sm-8">
            <input type="text" value="<%= @parentLabel%>" class="form-control" readonly>
          </div>
        </div>
        <div class="form-group">
          <label for="inputEmail3" class="col-sm-2 control-label">名称</label>
          <div class="col-sm-8">
            <%= f.text_field :name,class:"form-control",placeholder:"名称" %>
          </div>
        </div>
        <div class="form-group">
          <div class="col-sm-offset-2 col-sm-8">

          <%= f.submit '保 存',class:'btn btn-primary'%>
          </div>
        </div>
      <% end %>
